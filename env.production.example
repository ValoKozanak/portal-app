# Produkčné nastavenia
NODE_ENV=production

# Backend API URL (budete musieť zmeniť na skutočnú URL)
REACT_APP_API_URL=https://your-backend-url.railway.app

# Dropbox Configuration (budete musieť nastaviť skutočné kľúče)
REACT_APP_DROPBOX_APP_KEY=your_dropbox_app_key_here
REACT_APP_DROPBOX_APP_SECRET=your_dropbox_app_secret_here
REACT_APP_DROPBOX_REDIRECT_URI=https://your-frontend-url.vercel.app/dropbox-callback

# Email Configuration
SENDGRID_API_KEY=your_sendgrid_api_key_here
FROM_EMAIL=noreply@yourdomain.com

# JWT Secret (vygenerujte silný kľúč)
JWT_SECRET=your-super-secure-jwt-secret-key-here

# CORS Origin
CORS_ORIGIN=https://your-frontend-url.vercel.app
